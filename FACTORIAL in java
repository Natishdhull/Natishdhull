import java.util.*;
public class factorial {
    public static int name(int a) {
       int f=1;
       for(int i=1;i<=a;i++) {
        f=f*i;
       }
        return f;
    }
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        System.out.println("ENTER UR NUMBER");
        int a= sc.nextInt();
        System.out.println(name(a));
    }
}
